<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:paddingHorizontal="30dp"
        android:paddingVertical="20dp">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:orientation="vertical"
                >


                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="50dp"
                    android:orientation="horizontal"
                    android:textColor="@color/white">


                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="1.1">

                    <TextView
                        android:id="@+id/contain1"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:fontFamily="@font/lottemartd_medium"
                        android:text="가장 많이 방문한 장소는?"
                        android:textColor="@color/black"
                        android:textSize="30dp"
                        android:gravity="left"
                        android:textFontWeight="600"/>


                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="0.7"
                    android:orientation="vertical">

                    <LinearLayout
                        android:id="@+id/contain2"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:orientation="horizontal"
                        android:gravity="center|left">

                        <TextView
                            android:fontFamily="@font/lottemartd_medium"
                            android:textSize="18dp"
                            android:textColor="#2196F3"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="동물원 " />

                        <TextView
                            android:fontFamily="@font/lottemartd_medium"
                            android:textSize="18dp"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="에서" />

                        <TextView
                            android:fontFamily="@font/lottemartd_medium"
                            android:textSize="18dp"
                            android:textColor="#2196F3"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text=' "5회" ' />

                        <TextView
                            android:fontFamily="@font/lottemartd_medium"
                            android:textSize="18dp"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="데이트 했어요" />


                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/contain3"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:orientation="horizontal"
                        android:gravity="center|left">

                        <TextView
                            android:fontFamily="@font/lottemartd_medium"
                            android:textSize="18dp"
                            android:textColor="#2196F3"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="카페 " />

                        <TextView
                            android:fontFamily="@font/lottemartd_medium"
                            android:textSize="18dp"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="에서" />

                        <TextView
                            android:fontFamily="@font/lottemartd_medium"
                            android:textSize="18dp"
                            android:textColor="#2196F3"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text=' "4회" ' />

                        <TextView
                            android:fontFamily="@font/lottemartd_medium"
                            android:textSize="18dp"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="데이트 했어요" />


                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/contain4"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:orientation="horizontal"
                        android:gravity="center|left">

                        <TextView
                            android:fontFamily="@font/lottemartd_medium"
                            android:textSize="18dp"
                            android:textColor="#2196F3"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="식당 " />

                        <TextView
                            android:fontFamily="@font/lottemartd_medium"
                            android:textSize="18dp"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="에서" />

                        <TextView
                            android:fontFamily="@font/lottemartd_medium"
                            android:textSize="18dp"
                            android:textColor="#2196F3"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text=' "1회" ' />

                        <TextView
                            android:fontFamily="@font/lottemartd_medium"
                            android:textSize="18dp"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="데이트 했어요" />


                    </LinearLayout>

                </LinearLayout>


                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:orientation="vertical">


                    <TextView
                        android:id="@+id/contain5"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:gravity="center|bottom"
                        android:text='" 동물원에서 "4회" 데이트 했어요 "'
                        android:textColor="#04A3F8" />

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:text="동물원" />


                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:orientation="vertical">


                </LinearLayout>


            </LinearLayout>


        </LinearLayout>
    </RelativeLayout>

</androidx.constraintlayout.widget.ConstraintLayout>